<?xml version="1.0" encoding="UTF-8"?>
<controls:SafeAreaViewCell xmlns="http://xamarin.com/schemas/2014/forms" 
                           xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                           xmlns:controls="clr-namespace:OpenGeoDB.Core.Controls;assembly=OpenGeoDB.Core"
                           x:Class="OpenGeoDB.Core.Controls.DetailActionViewCell">
    
  <ViewCell.View>
      <StackLayout x:Name="CellLayout" Orientation="Vertical" Spacing="0" VerticalOptions="Center">
          <Label x:Name="TextLabel" />
          <Label x:Name="DetailLabel" LineBreakMode="TailTruncation" />
      </StackLayout>
  </ViewCell.View>
</controls:SafeAreaViewCell>