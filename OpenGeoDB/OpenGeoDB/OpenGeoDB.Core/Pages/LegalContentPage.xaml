                    x:Class="OpenGeoDB.Core.Pages.LegalContentPage" x:TypeArguments="vm:LegalContentViewModel"
                    xmlns:vm="clr-namespace:OpenGeoDB.Core.ViewModels;assembly=OpenGeoDB.Core"
                    xmlns:mvx="clr-namespace:MvvmCross.Forms.Views;assembly=MvvmCross.Forms"
                    xmlns:controls="clr-namespace:OpenGeoDB.Core.Controls;assembly=OpenGeoDB.Core"
                    Title="{Binding Title}" x:Name="Page">

    <controls:OfflineWebView OfflineContent="{Binding Content}" />
</mvx:MvxContentPage>